<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="NonActiveBorderColor">#FFCCCEDB</Color>
    <Color x:Key="InverseControlBorderColor">#3f3f46</Color>

    <SolidColorBrush x:Key="InverseControlBorderBrush" Color="{StaticResource InverseControlBorderColor}"/>
    <SolidColorBrush x:Key="NonActiveGlowBrush" Color="{StaticResource NonActiveBorderColor}"/>
</ResourceDictionary>